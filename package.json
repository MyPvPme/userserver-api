{
  "name": "userserver-api",
  "version": "1.32.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "18.*.*",
    "npm": "8.19.*"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:all": "nest build rest-api && nest build container-api && nest build files-api",
    "start:rest-api": "nest start rest-api",
    "start:rest-api:dev": "nest build rest-api --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:rest-api:debug": "nest start rest-api --webpack --webpackPath webpack-hmr.config.js --debug --watch",
    "start:rest-api:prod": "node dist/apps/rest-api/main.js",
    "start:files-api": "nest start files-api",
    "start:files-api:dev": "nest start files-api --watch",
    "start:files-api:debug": "nest start files-api --debug --watch",
    "start:files-api:prod": "node dist/apps/files-api/main.js",
    "start:container-api": "nest start container-api",
    "start:container-api:dev": "nest start container-api --watch",
    "start:container-api:debug": "nest start container-api --debug --watch",
    "start:container-api:prod": "node dist/apps/container-api/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:test": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "format:test": "prettier -c \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/userserver-api/test/jest-e2e.json",
    "pregenerate-clients": "sh ./generation-utils/init-clients.sh",
    "generate-clients": "sh ./generation-utils/generate-clients.sh",
    "postgenerate-clients": "sh ./generation-utils/push-clients.sh",
    "pregenerate-clients:test": "sh ./generation-utils/init-clients.sh",
    "generate-clients:test": "sh ./generation-utils/generate-clients.sh"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.8.14",
    "@grpc/proto-loader": "0.7.6",
    "@influxdata/influxdb-client": "1.33.2",
    "@mypvp/base-rest-client-nodejs": "1.3.0",
    "@mypvp/userserver-api-auth": "3.4.1",
    "@nestjs/common": "9.4.2",
    "@nestjs/core": "9.4.2",
    "@nestjs/event-emitter": "1.4.1",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/microservices": "9.4.2",
    "@nestjs/platform-express": "9.4.2",
    "@nestjs/platform-socket.io": "9.4.2",
    "@nestjs/schedule": "2.2.1",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/typeorm": "9.0.1",
    "@nestjs/websockets": "9.4.2",
    "@sentry/node": "7.49.0",
    "@sentry/tracing": "7.49.0",
    "adm-zip": "0.5.10",
    "archiver": "5.3.1",
    "aws-sdk": "2.1362.0",
    "cache-manager": "4.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "1.4.6",
    "crc-32": "1.2.2",
    "dockerode": "3.3.4",
    "dot-properties": "1.0.1",
    "fs-extra": "11.1.1",
    "jwks-rsa": "3.0.1",
    "mime-types": "2.1.35",
    "mysql2": "2.3.3",
    "node-stream-zip": "1.15.0",
    "pngjs": "7.0.0",
    "raw-body": "2.5.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.8.0",
    "swagger-ui-express": "4.6.2",
    "typeorm": "0.3.15",
    "typeorm-naming-strategies": "4.1.0",
    "yaml": "2.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.11",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.2",
    "@openapitools/openapi-generator-cli": "2.7.0",
    "@types/adm-zip": "0.5.0",
    "@types/archiver": "5.3.2",
    "@types/cache-manager": "4.0.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cron": "2.0.1",
    "@types/dockerode": "3.3.12",
    "@types/express": "4.17.17",
    "@types/fs-extra": "11.0.1",
    "@types/jest": "29.5.3",
    "@types/mime-types": "2.1.1",
    "@types/multer": "1.4.7",
    "@types/node": "18.17.1",
    "@types/pngjs": "6.0.1",
    "@types/properties-reader": "2.1.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-import": "2.28.0",
    "jest": "29.6.2",
    "prettier": "2.8.8",
    "run-script-webpack-plugin": "0.2.0",
    "semver": "7.5.4",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@mypvp/userserver-api-services(|/.*)$": "<rootDir>/libs/services/src/$1",
      "^@app/type-utils(|/.*)$": "<rootDir>/libs/type-utils/src/$1"
    }
  }
}
